<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorControlActivated">@color/ugly_green</item>
        <item name="android:textSize">5dp</item>
        <item name="android:textColor">@drawable/calender_date_selector</item>
        <item name="android:textColorPrimary">@color/white</item>


    </style>
    <style name="CalenderViewDateCustomText">
        <item name="android:fontFamily">@font/montserrat_semibold</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@drawable/calender_date_selector</item>
    </style>

    <style name="CalenderViewWeekCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/montserrat_semibold</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="CalendarHeaderYearText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">18dp</item>
        <item name="android:fontFamily">@font/montserrat_semibold</item>
        <item name="android:textColor">@color/white</item>
    </style>




</resources>